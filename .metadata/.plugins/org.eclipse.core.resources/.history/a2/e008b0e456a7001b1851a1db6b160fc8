
public class PlusOne {

	public static void main(String[] args) {
		int arr[]= {9,9};
		plusOne(arr);

	}
	
	public static int[] plusOne(int[]arr) {
		for(int i=arr.length-1;i>=0;i--) {
			if(arr[i]<9)
				arr[i]++;
			else {
				arr[i]=0;
			}
		}
		int result[] = new int[arr.length+1];
		result[0]=1;
		return result;
	}

}
