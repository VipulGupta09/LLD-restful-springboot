package dp.tree;

public class BurnTree {

	public static void main(String[] args) {
		TreeNode node = new TreeNode(new TreeNode(), new TreeNode(), 1);
		node.left = new TreeNode(new TreeNode(), new TreeNode(), 2);
		node.right = new TreeNode(new TreeNode(), new TreeNode(), 3);
		node.right.left = new TreeNode(new TreeNode(), new TreeNode(), 4);
		node.right.right = new TreeNode(new TreeNode(),null,5);
		node.right.left.left = new TreeNode(new TreeNode(), new TreeNode(), 6);
		node.right.left.right = new TreeNode(new TreeNode(), new TreeNode(), 7);
		node.right.right.left = new TreeNode(new TreeNode(),null,8);
		node.right.right.right.right = new TreeNode(new TreeNode(),null,9);

	}

}
