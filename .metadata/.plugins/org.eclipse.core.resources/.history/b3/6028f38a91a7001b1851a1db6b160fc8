public class HashMapBasedStorage<Key, Value> implements Storage<Key, Value> {
	
	Map<Key, Value> storage;
	private final Integer capacity;
	
		public HashMapBasedStorage(Integer capacity) {
			this.capacity = capacity;
			storage = new HashMap<>();
		}

	@Override
