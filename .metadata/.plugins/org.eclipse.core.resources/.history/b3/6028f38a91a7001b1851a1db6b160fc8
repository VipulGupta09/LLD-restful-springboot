		// TODO Auto-generated method stub

	}

	@Override
	public void remove(Key key) throws NotFoundException {
		// TODO Auto-generated method stub

	}

	@Override
	public void get(Key key) throws NotFoundException {
		// TODO Auto-generated method stub

	}

}
